				    <bt:Image size="32" resid="drawio.editIcon_32x32" />
				    <bt:Image size="80" resid="drawio.editIcon_80x80" />
				  </Icon>
				  <Action xsi:type="ExecuteFunction">
				    <FunctionName>editSelImage</FunctionName>
				  </Action>
				</Control>
              </Group>
            </OfficeTab>
          </ExtensionPoint>
