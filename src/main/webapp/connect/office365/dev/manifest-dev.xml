    <AppDomain>https://login.microsoftonline.com</AppDomain>
    <AppDomain>https://exp.draw.io</AppDomain>
    <AppDomain>https://graph.microsoft.com</AppDomain>
    <AppDomain>https://test.draw.io</AppDomain>
  </AppDomains>
  <!--End Basic Settings. -->

